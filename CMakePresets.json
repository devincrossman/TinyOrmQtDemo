{
  "version": 4,
  "include": [
    "build/generators/CMakePresets.json"
  ],
  "configurePresets": [
    {
      "name": "app-config-base",
      "inherits": "conan-default"
    }
  ],
  "buildPresets": [
    {
      "name": "app-RelWithDebInfo",
      "configurePreset": "app-config-base",
      "configuration": "RelWithDebInfo",
      "jobs": 4
    },
    {
      "name": "app-Release",
      "configurePreset": "app-config-base",
      "configuration": "Release",
      "jobs": 4
    }
  ]
}